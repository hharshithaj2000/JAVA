package javaAssignments;

public class arrays {
	public static void main(String[] args){
		int[] arr1={1,2,3};
		double[] arr2={2,3,4};
		String[] arr3={"hello","welcome","bye"};
		
		int[][] arr4={{1,2,3},{4,5,6}};
		double[][] arr5={{1,2,3},{4,5,6}};
		String[][] arr6={{"abc","def"},{"ghi","jkl"}};
		}
}
