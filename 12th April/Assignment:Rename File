package javaAssignments;

import java.io.*;

public class RenameFile {

	public static void main(String[] args) {
		File from=new File("D://fromname.txt");
		File to=new File("D://toname.txt");
		boolean flag=from.renameTo(to);
		if(flag==true){
			System.out.println("File name renamed");
		}
		else{
			System.out.println("File name not renamed");

		}
	}

}
