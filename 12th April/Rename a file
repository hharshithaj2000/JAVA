package files;

import java.io.*;

public class FileMain44 {
	public static void main(String[] args){
		File oldname=new File("D:\\details.txt");
		File newname=new File("D:\\Mydetails.txt");
		boolean flag=oldname.renameTo(newname);
		if(flag==true){
			System.out.println("File is renamed");
		}
		else{
			System.out.println("File is not renamed");
		}
	}
}
