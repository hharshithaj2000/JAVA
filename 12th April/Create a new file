package files;

import java.io.*;

public class FileMainn {

	public static void main(String[] args) {
		File newFile=new File("D:\\newFile.txt");
		try{
			boolean flag=newFile.createNewFile();
			if(flag==true){
				System.out.println("New file Created");
			}
			else{
				System.out.println("File already exists");
			}
		}
		catch(Exception e){
			e.printStackTrace();
		}
		

	}
}
