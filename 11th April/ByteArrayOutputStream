package io;

import java.io.*;

public class OpByteArr2 {
	public static void main(String[] args){
		String data="this is a code on byte array output stream";
		try{
			ByteArrayOutputStream out=new ByteArrayOutputStream();
			byte[] dataArr=data.getBytes();
			out.write(dataArr);
			String outStream=out.toString();
			System.out.println(outStream);
		}
		catch(Exception e){
			e.printStackTrace();
		}
		
	}
}
